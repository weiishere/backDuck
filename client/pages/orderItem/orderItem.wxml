<view class='container order'>
  <view class='order_header'>
    <view class="order_header_main">
      <image src='data:image/png;base64,'>
      </image>
      <text>{{stateDes}}</text>
    </view>

  </view>
  <view class='order_header_info'>
    <view>
      <image 
        src="data:image/png;base64,"></image>
    </view>
    <view>
      <view>
        <text>{{namemobile}}</text>
      </view>
      <view class='gray'>
        <text>{{address}}</text>
      </view>
    </view>
  </view>

  <view class='order_header_info info2'>
    <view>
      <view>
        <text>订单号 {{orderNo}}</text>
      </view>
      <view>
        <text>下单时间：{{createTime}}</text>
      </view>
    </view>
  </view>

  <view class='order_body'>
    <view class='order_item'>

      <view class='order_item_body'
        wx:for="{{orderItems}}"
        wx:for-item="item"
        wx:for-idx="idx"
        wx:key="{{idx}}">
        <view>
          <image src=""></image>
        </view>
        <view>
          <text>
              {{item.itemTitle}}
          </text>
          <text class='gray'>
              服务项目：{{item.businessType}}
          </text>
        </view>
        <view>
          <text>￥ {{item.realPrice}}</text>
          <!-- <text class='gray delete'>￥ 89.00</text> -->
        </view>
      </view>
      <view class='order_item_footer'>
        <view>
          <text>共{{orderItems.length}}件商品</text>
        </view>
      </view>
    </view>
  </view>

  <view class='order_footer'>
    <view>
      <text>金额合计：</text><text class='red'>￥ {{realOrderMoney}}</text>
    </view>
    <view>
    <text>确认签收</text>
    </view>
  </view>
</view>