<!--pages/home/home.js.wxml-->
<view class='container home'>
  <view class='warpview' bindtap="sideBarTrggle" data-sider-state='false' >
    <view class='home_header' >
      <view>
        <span>
          <i data-sider-state='true' catchtap="sideBarTrggle" class='icon-nav_menu'></i>
        </span>
      </view>
      <view>
        <span>
          <i class='icon-nav_orders'></i>
        </span>
      </view>
    </view>
    <view class='home_footer'>
      <view>
        <span>
          <image src='data:image/png;base64,'></image>
        </span>
        <text>上门取件</text>
      </view>
      <view>
        <span>
          <image src='data:image/png;base64,'></image>
        </span>
        <text>服务项目</text>
      </view>
      <view>
        <span>
          <image src='data:image/png;base64,'></image>
        </span>
        <text>品牌介绍</text>
      </view>
    </view>
  </view>

  <view class="home_sider {{sideBarShow?'show':''}}">
    <view class='home_sider_header'>
      <image src='https://xiu-store-1255815795.cos.ap-chengdu.myqcloud.com/blackDuck/ZmLXp4.jpg'></image>
      <view class='home_sider_name'>
        <text>黑鸭子</text>
      </view>
      <view class='home_sider_level'>
        <image src='https://xiu-store-1255815795.cos.ap-chengdu.myqcloud.com/blackDuck/vip_diamond%403x.png'></image>
        <text>黄金会员</text>
      </view>
    </view>
    <view class='home_sider_body'>
      <ul>
        <li>
          <i class='icon-leftbar_icon_events'></i>
          <text>最新活动</text>
        </li>
        <li catchtap="gotoPage" data-order='../order/index'>
          <i class='icon-nav_orders'></i>
          <text>我的订单</text>
        </li>
        <li>
          <i class='icon-leftbar_icon_clock'></i>
          <text>我的预约</text>
        </li>
        <li>
          <i class='icon-leftbar_icon_wallet'></i>
          <text>账户余额</text>
        </li>
        <li>
          <i class='icon-leftbar_icon_mail' style="font-size:26rpx"></i>
          <text>我的消息</text>
        </li>
        <li>
          <i class='icon-leftbar_icon_add'></i>
          <text>我的地址</text>
        </li>
      </ul>
    </view>
    <view class='home_sider_footer'>
      <view>
        <i class='icon-leftbar_icon_service'></i>
        <text>客服</text>
      </view>
      <view>
        <i class='icon-leftbar_icon_advice'></i>
        <text>意见与建议</text>
      </view>
    </view>
  </view>
</view>