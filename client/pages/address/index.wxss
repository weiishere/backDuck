page{
  background: #f1f1f1;
}
.address-list{
  padding-bottom: 120rpx
  
}
.address-list .address-item{
  background: #fff;
  padding: 0 30rpx;
  margin-top: 18rpx;
}
.address-item .address-info{
  font-size: 30rpx;
  padding: 30rpx 24rpx;
  border-bottom: #aeaeae 1rpx solid;
}
.address-item .address-info .address{
  font-size: 24rpx;
  color: #646464;
  margin-top: 12rpx;
}
.address-item .address-tools{
  display: flex;
  padding: 30rpx 0;
}
.address-item .address-tools .setdefault{
  flex: 1;
  position: relative;
  padding-left: 50rpx;
}
.address-tools .setdefault .default-icon {
  width: 44rpx;
  height: 44rpx;
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 44rpx;
}
.address-tools .setdefault .default-icon.active {
  background: url('data:image/png;base64,') no-repeat;
  background-size: 44rpx;
}
.address-item .address-tools .setedit{
  background: url('data:image/png;base64,') no-repeat left center;
  background-size: 44rpx;
  padding-left: 50rpx;
  margin-left: 40rpx;
}
.address-item .address-tools .setdelete{
  background: url('data:image/png;base64,') no-repeat left center;
  background-size: 44rpx;
  padding-left: 50rpx;
  margin-left: 40rpx;
}
.address-wrap .add-address{
  background: #235379;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  width: 100%;
  position: fixed;
  bottom: 0px;
  color: #fff;
}