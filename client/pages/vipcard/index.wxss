.vipcard-top{
  width: 100%;
  height: 370rpx;
  position: relative;
  background: #3b3a3b;
}
.vipcard-top .user-info-wrap{
  display: flex;
  position: absolute;
  left: 65rpx;
  top: 116rpx;
}
.vipcard-top .user-info{
  display: flex;
  justify-content: center;
  align-items: center;
}
.vipcard-top .user-info .signin{
  margin-left: 26rpx;
  font-size: 30rpx;
  color: #b0986a;
}
.vipcard-top .user-info .signin .user-name{
  font-size: 30rpx;
  color: #fff;
}
.vipcard-top .user-info .signin .user-type{
  font-size: 22rpx;
  color: #b0986a;
  margin-top: 15rpx;
}
.vipcard-top .user-icon-wrap {
}
.vipcard-top .user-icon-wrap .user-icon{
  width: 100rpx;
  height: 100rpx;
  display: inline-block;
  border-radius: 99px;
}
.vipcard-top .vipcard-icon-wrap{
  width: 116rpx;
  height: 158rpx;
  position: absolute;
  top: 92rpx;
  right: 75rpx;
}
.vipcard-top .vipcard-icon-wrap .vipcard-icon{
  display: block;
  width: 100%;
  height: 100%;
}
.vipcard-top .login-btn{
  width: 126rpx;
  height: 50rpx;
  color: #b0986a;
  line-height: 50rpx;
  text-align: center;
  border-radius: 5px;
  border: #b0986a solid 1rpx;
  position: absolute;
  bottom: 30rpx;
  right: 65rpx;
  font-size: 28rpx;
}
.title{
  height: 90rpx;
  line-height: 90rpx;
  background: #f5f5f9;
  position: relative;
  padding: 0 80rpx;
}
.title .title-icon{
  width: 44rpx;
  height: 44rpx;
  display: inline-block;
  position: absolute;
  left: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}
.vipcard-desc{
  display: block;
  line-height: 62rpx;
  margin: 30rpx;
}
.vipcard-desc tr {
  display: flex;
  background: #eeeeee;
}
.vipcard-desc tr:nth-of-type(odd){
  background: #e5e5e5;
}
.vipcard-desc th,
.vipcard-desc td {
  flex: 1
}
.vipcard-desc th{
  background: #235379;
  color: #fff;
}
.vipcard-desc tr td,
.vipcard-desc tr th{
  text-align: center
}
.vipcard-desc th:first-child,
.vipcard-desc tr td:first-child{
  padding: 0 0 0 18rpx;
  white-space: nowrap;
}
.vipcard-desc tr td:first-child{
  color: #235379;
  text-align: left;
}
.vipcard-tip{
  text-align: center;
  color: #535353;
}