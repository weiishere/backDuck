@import '/utils/form.wxss';
page {
  background: #fff;
}
.address-wrap{}
.address-wrap .form-wrap{
  background: #fff;
  padding: 0 30rpx;
}
.address-wrap .form-wrap  .form-item:last-child{
  border: none;
}
.form-wrap .form-item .form-item-label .label{
  width: 200rpx;
}

.form-item-tools .arrow-icon {
  width: 44rpx;
  height: 44rpx;
  display: block;
  left: 0;
  top: 0;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 44rpx;
}